{
  "name": "db",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "run-s generate build:db",
    "build:db": "tsup",
    "dev": "run-s generate dev:db",
    "dev:db": "tsup --watch",
    "dev:pscale": "pscale connect plutos dev --port 3309",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "db:format": "prisma format",
    "generate": "prisma generate",
    "lint": "eslint \"src/**/*.ts\"",
    "studio": "prisma studio",
    "clean": "rimraf .turbo node_modules dist"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "eslint": "^8.38.0",
    "eslint-config-custom": "workspace:*",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.13.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
